<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/goto_ar_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_ar_gnss" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/goto_animations_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_animations" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/goto_compass_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_compass" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/goto_layout_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_coordinator_layout" />

    <TextView
        android:id="@+id/clock_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

</merge>
