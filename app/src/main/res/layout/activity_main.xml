<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.kylemadsen.testandroid.MainActivity">

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/goto_gnss_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_goto_gnss"/>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/goto_ar_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_ar_gnss"/>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/goto_animations_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/activity_main_animations"/>
</LinearLayout>
